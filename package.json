  "description": "My site.",
  "keywords": [
    "Bickley-Wallace",
    "Josh",
    "Site"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jayseeare/bickleywallace-site.git"
  },
  "bugs": {
    "url": "https://github.com/jayseeare/bickleywallace-site/issues"
  },
  "homepage": "https://github.com/jayseeare/bickleywallace-site#readme",
  "scripts": {
    "bootstrap:ci": "lerna bootstrap --hoist -- --production",
    "bootstrap": "lerna bootstrap --hoist",
    "build": "lerna run --stream build",
    "clean": "git clean -fdX",
    "e2e": "cd packages/e2e && cypress",
    "gen-certs": "./scripts/gen-certs.sh",
    "lint": "lerna run --parallel --no-bail lint",
    "nvm": "nvm use 12.18.0",
    "start": "lerna run --parallel start",
    "test:watch": "lerna run --parallel test:watch",
    "test": "lerna run --parallel --no-bail test",
    "type-check": "lerna run --parallel --no-bail type-check"
  },
  "devDependencies": {
    "lerna": "^3.22.1"
  }
}
