{
  "name": "jaybeeuu-dev",
  "version": "0.0.0",
  "author": "Josh Bickley-Wallace <joshbickleywallace@outlook.com> (https://github.com/jaybeeuu)",
  "license": "MIT",
  "description": "My site.",
  "keywords": [
    "Bickley-Wallace",
    "Josh",
    "Site"
  ],
  "engines": {
    "node": ">=14.16.0",
    "pnpm": ">=6.2.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaybeeuu/bickleywallace-site.git"
  },
  "bugs": {
    "url": "https://github.com/jaybeeuu/bickleywallace-site/issues"
  },
  "homepage": "https://github.com/jaybeeuu/bickleywallace-site#readme",
  "scripts": {
    "bootstrap": "pnpm install -r",
    "build": "pnpm run -r --stream build --filter {packages}",
    "build-changes": "pnpm run -r --stream build --filter {packages}[HEAD]",
    "clean": "pnpx rimraf ./node_modules ./packages/*/node_modules & git clean -fdX",
    "e2e": "pnpm run -r e2e --filter @jaybeeuu/e2e",
    "e2e-ci": "pnpm run start --filter @jaybeeuu/client & pnpx wait-on --timeout=60000 https://localhost:3443/ && pnpm run e2e run",
    "gen-certs": "pnpm run --no-bail --no-sort -r gen-certs --filter {packages}",
    "lint": "pnpm run --no-bail --no-sort -r lint --filter {packages}",
    "nvm": "nvm use 14.15.0",
    "spellcheck": "pnpm run --no-bail --no-sort -r spellcheck --filter {packages}",
    "start": "pnpm run -r start --no-sort --workspace-concurrency=Infinity --filter @jaybeeuu/client...",
    "test": "jest",
    "test:watch": "jest --watch",
    "type-check": "pnpm run --no-bail --no-sort -r type-check --filter {packages}"
  },
  "devDependencies": {
    "jest": "^27.0.1"
  }
}
