{
  "name": "@jaybeeuu/utilities",
  "version": "5.0.3",
  "author": "Josh Bickley-Wallace <joshbickleywallace@outlook.com> (https://github.com/jaybeeuu)",
  "license": "MIT",
  "description": "The utility functions used by my application",
  "keywords": [
    "Bickley-Wallace",
    "Josh",
    "Utilities"
  ],
  "homepage": "https://github.com/jaybeeuu/jaybeeuu-dev/tree/main/packages/utilities#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/jaybeeuu/jaybeeuu-dev.git"
  },
  "bugs": {
    "url": "https://github.com/jaybeeuu/jaybeeuu-dev/issues"
  },
  "type": "module",
  "sideEffects": false,
  "files": [
    "lib"
  ],
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    "./test": {
      "default": "./lib/test/index.js"
    },
    ".": {
      "default": "./lib/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "test": [
        "./lib/test"
      ]
    }
  },
  "scripts": {
    "build": "tsc --project ./tsconfig.build.json --pretty",
    "lint": "eslint --format codeframe . --max-warnings 0",
    "start": "tsc --project ./tsconfig.build.json --pretty --watch --incremental",
    "test": "jest",
    "test:watch": "jest --watch  --no-cache --runInBand",
    "type-check": "tsc --noEmit --pretty"
  },
  "dependencies": {
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@jaybeeuu/eslint-config": "workspace:*",
    "@jaybeeuu/is": "workspace:*",
    "@jest/globals": "^30.1.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "eslint": "^9.34.0",
    "eslint-plugin-jest": "^29.0.1",
    "jest": "^30.1.1",
    "ts-jest": "^29.4.1",
    "typescript": "^5.9.2"
  }
}
