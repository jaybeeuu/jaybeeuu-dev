{
  "name": "@jaybeeuu/utilities",
  "version": "0.0.1",
  "author": "Josh Bickley-Wallace <joshbickleywallace@outlook.com> (https://github.com/jaybeeuu)",
  "license": "MIT",
  "description": "The utility functions used by my application",
  "keywords": [
    "Bickley-Wallace",
    "Josh",
    "Utilities"
  ],
  "type": "module",
  "sideEffects": false,
  "files": [
    "lib"
  ],
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    "./test": {
      "default": "./lib/test/index.js"
    },
    ".": {
      "default": "./lib/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "test": ["./lib/test"]
    }
  },
  "scripts": {
    "build": "tsc --project ./tsconfig.build.json --pretty",
    "lint": "eslint --ext .js,.ts --format codeframe .",
    "start": "tsc --project ./tsconfig.build.json --pretty --watch --incremental",
    "test": "jest",
    "test:watch": "jest --watch  --no-cache --runInBand",
    "type-check": "tsc --noEmit --pretty"
  },
  "dependencies": {
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@jaybeeuu/eslint-config": "workspace:*",
    "@types/jest": "^28.1.6",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "eslint": "^8.21.0",
    "eslint-plugin-jest": "^26.7.0",
    "jest": "^28.1.3",
    "ts-jest": "^28.0.7",
    "typescript": "^4.7.4"
  }
}
