{
  "name": "@jaybeeuu/e2e",
  "version": "0.0.0",
  "author": "Josh Bickley-Wallace <joshbickleywallace@outlook.com> (https://github.com/jaybeeuu)",
  "license": "MIT",
  "description": "E2E tests for my blog.",
  "keywords": [
    "Bickley-Wallace",
    "Josh",
    "E2E"
  ],
  "scripts": {
    "build": "compost --source-dir ./fixtures/src/posts --output-dir ./fixtures/posts --href-root posts",
    "postbuild": "node ./fixtures/src/posts/post-processing.mjs",
    "e2e": "cypress",
    "lint": "eslint --ext .js,.ts --format codeframe .",
    "type-check": "tsc --noEmit --pretty"
  },
  "dependencies": {
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.8",
    "@jaybeeuu/compost": "workspace:*",
    "@jaybeeuu/e2e-hooks": "workspace:*",
    "@jaybeeuu/eslint-config": "workspace:*",
    "@jaybeeuu/utilities": "workspace:*",
    "@types/node": "^14.17.0",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "core-js": "3.16.0",
    "cypress": "8.1.0",
    "eslint": "^7.32.0",
    "eslint-plugin-cypress": "^2.11.3",
    "typescript": "^4.3.5"
  }
}
