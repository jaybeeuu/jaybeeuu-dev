  "description": "The API behind my blog",
  "keywords": [
    "Bickley-Wallace",
    "Josh",
    "UI"
  ],
  "main": "./src/index.ts",
  "scripts": {
    "build": "babel --extensions .ts --out-dir dist ./src",
    "start": "nodemon --inspect ./src/index.ts",
    "start:prod": "node dist/index.js",
    "lint": "eslint --ext .js,.ts --format codeframe .",
    "type-check": "tsc --noEmit",
    "test": "jest --config ./test/jest.config.js",
    "test:watch": "node --inspect ./node_modules/.bin/jest --config ./test/jest.config.js --runInBand --watch --no-cache"
  },
  "dependencies": {
    "@types/cors": "^2.8.6",
    "@types/node": "^13.9.0",
    "@types/rimraf": "^2.0.3",
    "chalk": "^3.0.0",
    "cookie-parser": "^1.4.4",
    "core-js": "^3.6.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "morgan": "^1.9.1",
    "source-map-support": "^0.5.16"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-typescript": "^7.8.3",
    "@types/chalk": "^2.2.0",
