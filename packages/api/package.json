{
  "name": "@bickley-wallace/api",
  "version": "0.0.0",
  "author": "Josh Bickley-Wallace <joshbickleywallace@outlook.com> (https://github.com/jayseeare)",
  "license": "MIT",
  "description": "The API behind my blog",
  "keywords": [
    "Bickley-Wallace",
    "Josh",
    "API"
  ],
  "main": "./src/index.ts",
  "scripts": {
    "build": "babel --extensions .ts --out-dir dist ./src",
    "start": "nodemon ./src/index.ts",
    "start:debug": "nodemon --inspect ./src/index.ts",
    "start:prod": "node dist/index.js",
    "lint": "eslint --ext .js,.ts --format codeframe .",
    "type-check": "tsc --noEmit",
    "test": "jest --config ./test/jest.config.js",
    "test:watch": "node --inspect ./node_modules/.bin/jest --config ./test/jest.config.js --runInBand --watch --no-cache"
  },
  "dependencies": {
    "chalk": "^4.0.0",
    "cookie-parser": "^1.4.5",
    "core-js": "^3.6.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "highlight.js": "^9.18.1",
    "marked": "^0.8.2",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.0",
    "rmfr": "^2.0.0",
    "sanitize-html": "^1.23.0",
    "simple-git": "^1.132.0",
    "source-map-support": "^0.5.16"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.4",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-typescript": "^7.9.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.6",
    "@types/highlight.js": "^9.12.3",
    "@types/jest": "^25.2.1",
    "@types/marked": "^0.7.4",
    "@types/morgan": "^1.7.37",
    "@types/node": "13.11.1",
    "@types/node-fetch": "^2.5.6",
    "@types/nodegit": "^0.26.2",
    "@types/request-promise-native": "^1.0.17",
    "@types/rmfr": "^2.0.0",
    "@types/sanitize-html": "^1.22.0",
    "@types/source-map-support": "^0.5.1",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "eslint": "^6.8.0",
    "eslint-plugin-jest": "^23.6.0",
    "jest": "^25.3.0",
    "jest-date-mock": "^1.0.8",
    "nodemon": "^2.0.3",
    "typescript": "^3.7.5"
  }
}
