{
  "name": "@bickley-wallace/api",
  "version": "0.0.0",
  "author": "Josh Bickley-Wallace <joshbickleywallace@outlook.com> (https://github.com/jayseeare)",
  "license": "MIT",
  "description": "The API behind my blog",
  "keywords": [
    "Bickley-Wallace",
    "Josh",
    "UI"
  ],
  "main": "./src/index.ts",
  "scripts": {
    "build": "babel --extensions .ts --out-dir dist ./src",
    "start": "nodemon --inspect ./src/index.ts",
    "start:prod": "node dist/index.js",
    "lint": "eslint --ext .js,.ts --format codeframe .",
    "type-check": "tsc --noEmit",
    "test": "jest --config ./test/jest.config.js",
    "test:watch": "node --inspect ./node_modules/.bin/jest --config ./test/jest.config.js --runInBand --watch --no-cache"
  },
  "dependencies": {
    "@types/cors": "^2.8.6",
    "chalk": "^3.0.0",
    "cookie-parser": "^1.4.4",
    "core-js": "^3.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "morgan": "^1.9.1",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.8",
    "source-map-support": "^0.5.16"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/node": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "@types/chalk": "^2.2.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.2",
    "@types/jest": "^24.0.23",
    "@types/morgan": "^1.7.37",
    "@types/node": "^12.12.11",
    "@types/request-promise-native": "^1.0.17",
    "@types/source-map-support": "^0.5.0",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.8.0",
    "eslint": "^6.8.0",
    "eslint-plugin-jest": "^23.0.4",
    "jest": "^24.9.0",
    "nodemon": "^2.0.1",
    "typescript": "^3.7.5"
  }
}
