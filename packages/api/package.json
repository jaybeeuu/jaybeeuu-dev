    "@types/source-map-support": "^0.5.1",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "eslint": "^6.8.0",
    "eslint-plugin-jest": "^23.6.0",
    "jest": "^25.1.0",
    "nodemon": "^2.0.1",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.8",
    "rmfr": "^2.0.0",
    "typescript": "^3.7.5"
