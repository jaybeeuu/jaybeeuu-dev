{
  "name": "@jaybeeuu/compost",
  "version": "0.0.0",
  "author": "Josh Bickley-Wallace <joshbickleywallace@outlook.com> (https://github.com/jaybeeuu)",
  "license": "MIT",
  "description": "A CLI took for compiling the posts in my blog.",
  "keywords": [
    "Compile",
    "Posts"
  ],
  "type": "module",
  "engines": {
    "node": ">=14.16.0"
  },
  "bin": "bin/compost.js",
  "main": "lib/index.js",
  "files": [
    "bin",
    "lib"
  ],
  "scripts": {
    "build": "tsc --project ./tsconfig.build.json --pretty",
    "start": "tsc --project ./tsconfig.build.json --pretty --watch --incremental",
    "lint": "eslint --ext .js,.ts --format codeframe .",
    "type-check": "tsc --noEmit --pretty",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": "./lib/index.js"
  },
  "dependencies": {
    "@types/node-fetch": "^2.5.10",
    "chalk": "^4.1.1",
    "chokidar": "^3.5.1",
    "highlight.js": "^10.7.2",
    "marked": "^2.0.6",
    "node-fetch": "^2.6.1",
    "rmfr": "^2.0.0",
    "sanitize-html": "^2.4.0",
    "tslib": "^2.2.0",
    "yargs": "^17.0.1"
  },
  "devDependencies": {
    "@jaybeeuu/eslint-config": "workspace:*",
    "@jaybeeuu/utilities": "workspace:*",
    "@types/jest": "^26.0.23",
    "@types/marked": "^2.0.3",
    "@types/node": "15.6.1",
    "@types/rmfr": "^2.0.0",
    "@types/sanitize-html": "^2.3.1",
    "@types/yargs": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "eslint": "^7.27.0",
    "eslint-plugin-jest": "^24.3.6",
    "jest": "^27.0.1",
    "jest-date-mock": "^1.0.8",
    "jest-matcher-utils": "^27.0.1",
    "ts-jest": "^27.0.1",
    "typescript": "^4.3.2"
  }
}
