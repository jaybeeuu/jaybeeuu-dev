{
  "name": "@jaybeeuu/compost",
  "version": "0.0.0",
  "author": "Josh Bickley-Wallace <joshbickleywallace@outlook.com> (https://github.com/jaybeeuu)",
  "license": "MIT",
  "description": "A CLI took for compiling the posts in my blog.",
  "keywords": [
    "Compile",
    "Posts"
  ],
  "type": "module",
  "engines": {
    "node": ">=14.16.0"
  },
  "bin": "bin/compost.js",
  "main": "lib/index.js",
  "files": [
    "bin",
    "lib"
  ],
  "exports": {
    ".": "./lib/index.js"
  },
  "scripts": {
    "build": "tsc --project ./tsconfig.build.json --pretty",
    "start": "tsc --project ./tsconfig.build.json --pretty --watch --incremental",
    "lint": "eslint --ext .js,.ts --format codeframe .",
    "type-check": "tsc --noEmit --pretty",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "chokidar": "^3.5.2",
    "highlight.js": "^11.3.1",
    "marked": "^3.0.7",
    "node-fetch": "^3.0.0",
    "prismjs": "^1.25.0",
    "sanitize-html": "^2.5.2",
    "tslib": "^2.3.1",
    "yargs": "^17.2.1"
  },
  "devDependencies": {
    "@jaybeeuu/eslint-config": "workspace:*",
    "@jaybeeuu/utilities": "workspace:*",
    "@types/jest": "^27.0.2",
    "@types/marked": "^3.0.2",
    "@types/node": "^14.17.27",
    "@types/node-fetch": "^3.0.3",
    "@types/prismjs": "^1.16.6",
    "@types/sanitize-html": "^2.5.0",
    "@types/yargs": "^17.0.4",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "eslint": "^8.0.1",
    "eslint-plugin-jest": "^25.2.2",
    "jest": "^27.3.1",
    "jest-date-mock": "^1.0.8",
    "jest-matcher-utils": "^27.3.1",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4"
  }
}
