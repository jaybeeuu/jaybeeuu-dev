{
  "name": "@jaybeeuu/compost",
  "version": "0.0.1",
  "author": "Josh Bickley-Wallace <joshbickleywallace@outlook.com> (https://github.com/jaybeeuu)",
  "license": "MIT",
  "description": "A CLI took for compiling the posts in my blog.",
  "keywords": [
    "compile",
    "markdown",
    "posts"
  ],
  "type": "module",
  "engines": {
    "node": ">=14.16.0"
  },
  "bin": "bin/compost.js",
  "main": "lib/index.js",
  "files": [
    "bin",
    "lib"
  ],
  "exports": {
    ".": "./lib/index.js"
  },
  "scripts": {
    "build": "tsc --project ./tsconfig.build.json --pretty",
    "start": "tsc --project ./tsconfig.build.json --pretty --watch --incremental",
    "lint": "eslint --ext .js,.ts --format codeframe .",
    "type-check": "tsc --noEmit --pretty",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "chalk": "^5.2.0",
    "chokidar": "^3.5.3",
    "highlight.js": "^11.7.0",
    "marked": "^4.2.12",
    "node-fetch": "^3.3.1",
    "prismjs": "^1.29.0",
    "sanitize-html": "^2.10.0",
    "tslib": "^2.5.0",
    "yargs": "^17.7.1"
  },
  "devDependencies": {
    "@jaybeeuu/eslint-config": "workspace:*",
    "@jaybeeuu/utilities": "workspace:*",
    "@types/jest": "^29.4.2",
    "@types/marked": "^4.0.8",
    "@types/node": "^18.15.3",
    "@types/node-fetch": "^2.6.2",
    "@types/prismjs": "^1.26.0",
    "@types/sanitize-html": "^2.9.0",
    "@types/yargs": "^17.0.22",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "eslint": "^8.36.0",
    "eslint-plugin-jest": "^27.2.1",
    "jest": "^29.5.0",
    "jest-date-mock": "^1.0.8",
    "jest-matcher-utils": "^29.5.0",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  }
}
