{
  "name": "@jaybeeuu/compost",
  "version": "0.0.0",
  "author": "Josh Bickley-Wallace <joshbickleywallace@outlook.com> (https://github.com/jaybeeuu)",
  "license": "MIT",
  "description": "A CLI took for compiling the posts in my blog.",
  "keywords": [
    "compile",
    "markdown",
    "posts"
  ],
  "type": "module",
  "engines": {
    "node": ">=14.16.0"
  },
  "bin": "bin/compost.js",
  "main": "lib/index.js",
  "files": [
    "bin",
    "lib"
  ],
  "exports": {
    ".": "./lib/index.js"
  },
  "scripts": {
    "build": "tsc --project ./tsconfig.build.json --pretty",
    "start": "tsc --project ./tsconfig.build.json --pretty --watch --incremental",
    "lint": "eslint --ext .js,.ts --format codeframe .",
    "type-check": "tsc --noEmit --pretty",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "chalk": "^5.0.1",
    "chokidar": "^3.5.3",
    "highlight.js": "^11.6.0",
    "marked": "^4.0.18",
    "node-fetch": "^3.2.9",
    "prismjs": "^1.28.0",
    "sanitize-html": "^2.7.0",
    "tslib": "^2.4.0",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@jaybeeuu/eslint-config": "workspace:*",
    "@jaybeeuu/utilities": "workspace:*",
    "@types/jest": "^28.1.6",
    "@types/marked": "^4.0.3",
    "@types/node": "^18.0.6",
    "@types/node-fetch": "^2.6.2",
    "@types/prismjs": "^1.26.0",
    "@types/sanitize-html": "^2.6.2",
    "@types/yargs": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "eslint": "^8.20.0",
    "eslint-plugin-jest": "^26.6.0",
    "jest": "^28.1.3",
    "jest-date-mock": "^1.0.8",
    "jest-matcher-utils": "^28.1.3",
    "ts-jest": "^28.0.7",
    "typescript": "^4.7.4"
  }
}
