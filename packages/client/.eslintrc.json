{
  "extends": [
    "@bickley-wallace/eslint-config/base",
    "plugin:react/recommended"
  ],
  "env": {
    "browser": true
  },
  "plugins": [
    "react"
  ],
  "settings": {
    "react": {
      "pragma": "h",
      "version": "detect"
    },
    "import/resolver": {
      "webpack": {
        "config": "./webpack.config.js"
      }
    }
  },
  "rules": {
    "react/prop-types": "off"
  }
}
