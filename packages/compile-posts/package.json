{
  "name": "@bickley-wallace/compile-posts",
  "version": "0.0.0",
  "author": "Josh Bickley-Wallace <joshbickleywallace@outlook.com> (https://github.com/jayseeare)",
  "license": "MIT",
  "description": "A CLI took for compiling the posts in my blog.",
  "keywords": [
    "Compile",
    "Posts"
  ],
  "bin": "./lib/index.js",
  "main": "./lib/index.js",
  "files": [
    "./lib"
  ],
  "scripts": {
    "build": "tsc --project ./tsconfig.build.json",
    "start": "tsc --watch --preserveWatchOutput --project ./tsconfig.build.json",
    "lint": "eslint --ext .js,.ts --format codeframe .",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch  --no-cache --runInBand"
  },
  "dependencies": {
    "getopts": "^2.2.5",
    "highlight.js": "^9.18.1",
    "marked": "^0.8.2",
    "rmfr": "^2.0.0",
    "sanitize-html": "^1.23.0",
    "ts-jest": "^26.1.1"
  },
  "devDependencies": {
    "@bickley-wallace/config-eslint": "workspace:*",
    "@types/highlight.js": "^9.12.3",
    "@types/jest": "^26.0.4",
    "@types/marked": "^0.7.4",
    "@types/node": "13.11.1",
    "@types/rmfr": "^2.0.0",
    "@types/sanitize-html": "^1.22.0",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "eslint": "^6.8.0",
    "eslint-plugin-jest": "^23.6.0",
    "jest": "^26.1.0",
    "jest-date-mock": "^1.0.8",
    "jest-matcher-utils": "^26.1.0",
    "typescript": "^3.7.5"
  }
}
