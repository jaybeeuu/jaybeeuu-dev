{
  "name": "@jaybeeuu/posts",
  "version": "0.0.0",
  "author": "Josh Bickley-Wallace <joshbickleywallace@outlook.com> (https://github.com/jaybeeuu)",
  "license": "MIT",
  "private": true,
  "type": "module",
  "description": "My blog posts",
  "keywords": [
    "Bickley-Wallace",
    "Josh",
    "Posts"
  ],
  "files": [
    "lib"
  ],
  "scripts": {
    "start": "compost --watch --href-root blog --include-unpublished --remove-h1 --old-manifest-locator \"https://jaybeeuu.dev/blog/manifest.json\"",
    "lint": "markdownlint-cli2 \"**/*.md\" \"#node_modules\"",
    "spellcheck-interactive": "mdspell --en-GB --ignore-numbers \"**/*.md\" \"!**/node_modules/**/*.md\"",
    "spellcheck": "pnpm run spellcheck-interactive --report",
    "build": "compost --href-root blog --code-line-numbers --remove-h1 --old-manifest-locator \"https://jaybeeuu.dev/blog/manifest.json\"",
    "example": "nodemon --exec \"node ./node_modules/ts-node/dist/bin.js --esm\""
  },
  "devDependencies": {
    "@jaybeeuu/compost": "workspace:*",
    "@types/node": "^20.2.5",
    "@types/yargs": "^17.0.24",
    "markdown-spellcheck": "^1.3.1",
    "markdownlint-cli2": "^0.7.1",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3",
    "yargs": "^17.7.2"
  }
}
