{
  "extends": [
    "plugin:react/recommended",
    "@jaybeeuu/eslint-config/base",
    "@jaybeeuu/eslint-config/jest"
  ],
  "env": {
    "browser": true
  },
  "plugins": ["react"],
  "settings": {
    "react": {
      "pragma": "h",
      "version": "detect"
    },
    "import/resolver": {
      "webpack": {
        "config": "./webpack.config.js"
      }
    }
  },
  "rules": {
    "react/prop-types": "off"
  }
}
